{
  "compilerOptions": {
    "target": "es2022",
    "module": "NodeNext",
    "moduleDetection": "force",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "rootDir": "src",
    "outDir": "dist",
    "sourceMap": true
  }
}
